{
  "name": "<%= moduleName %>",
  "version": "0.0.1",
  "description": "<%= moduleDesc %>",
  "main": "dist/index.js",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "prepublish": "in-publish && npm run lint && npm test && npm run coverage && npm run compile || echo ''",
    "test": "NODE_ENV=test node_modules/.bin/mocha --compilers js:babel-core/register",
    "lint": "eslint src test",
    "build": "babel src --out-dir dist",
    "coverage": "babel-node node_modules/.bin/isparta cover node_modules/.bin/_mocha && node_modules/.bin/coveralls < coverage/lcov.info",
    "postcoverage": "rimraf coverage",
    "watch": "watch 'npm run build && npm run test' src test",
    "clean": "rimraf dist",
    "postpublish": "greenkeeper enable > /dev/null 2>&1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davewasmer/<%= moduleName %>.git"
  },
  "author": "Dave Wasmer <davewasmer@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davewasmer/<%= moduleName %>/issues"
  },
  "homepage": "https://github.com/davewasmer/<%= moduleName %>#readme",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-eslint": "^4.1.6",
    "coveralls": "^2.11.4",
    "eslint-config-airbnb": "^1.0.2",
    "eslint": "^1.10.1",
    "in-publish": "2.0.0",
    "isparta": "^4.0.0",
    "mocha": "^2.3.4",
    "must": "^0.13.1",
    "rimraf": "^2.4.2",
    "watch": "^0.16.0"
  },
  "dependencies": {
    "lodash": "^3.10.1"
  }
}
